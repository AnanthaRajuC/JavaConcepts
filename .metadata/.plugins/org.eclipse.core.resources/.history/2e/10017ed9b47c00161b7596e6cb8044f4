/**
 * @author anantharaju
 */
package oopocPackage;

public class ObjectAndClass 
{
	int id;	     //instance variable
	String name; //instance variable

	public static void main(String[] args) 
	{
		ObjectAndClass oac = new ObjectAndClass(); //Object Creation
		
		oac.id   = 1;
		oac.name = "Ronnie James Dio";
		
		System.out.println("ID: "+ oac.id);
		System.out.println("Name: "+ oac.name);
	}
}
