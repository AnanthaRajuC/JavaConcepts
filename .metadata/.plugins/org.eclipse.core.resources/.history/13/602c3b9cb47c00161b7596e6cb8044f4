/**
 * @author anantharaju
 */
package oopocPackage;

public class ObjectAndClass 
{
	int id;
	String name;

	public static void main(String[] args) 
	{
		ObjectAndClass oac = new ObjectAndClass();
		
		oac.id   = 1;
		oac.name = "Ronnie James Dio";
		
		System.out.println("ID: "+ oac.id);
		System.out.println("Name: "+ oac.name);

	}
}
